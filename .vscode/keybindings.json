// Поместите настраиваемые сочетания клавиш в этот файл, чтобы перезаписать клавиши по умолчанию.
[
  {
    "key": "ctrl+shift+alt+c",
    "command": "liveSass.command.oneTimeCompileSass"
  },
  {
    "key": "f8",
    "command": "workbench.action.tasks.runTask",
    "args": "debug"
  },
  {
    "key": "shift+ctrl+f8",
    "command": "workbench.action.tasks.runTask",
    "args": "release"
  },
  {
    "key": "shift+f8",
    "command": "workbench.action.tasks.runTask",
    "args": "clean"
  }
]